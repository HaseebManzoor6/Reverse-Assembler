1 byte words

# 2 bit opcode
# xxxxxx00 add
#       01 mul
#       10 sub
#       11 div

mask 0b11000000 {
    0b00 = addi ADDR mask 0b00110000 ADDR mask 0b00001100 INT mask 0b00000011
    0b01 = add ADDR mask 0b00110000 ADDR mask 0b00001100 ADDR mask 0b00000011
    0b10 = mul ADDR mask 0b00110000 ADDR mask 0b00001100 ADDR mask 0b00000011
}
